@using System.Configuration
@using System.Reflection
@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="IE=8" />
    <title>@(this.ViewBag.Title)</title>

    @* @Styles.Render("~/Content/css")*@

    <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/Bootstrapv3.0.3-632/Bootstrap.css"))" />
    <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/icode.min.css"))" />
    <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/ko.plus.css"))" />
    <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/toastr.css"))" />
    <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/typeahead.js-bootstrap.css"))" />
    <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/chosen.min.css"))"  />
    <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/smart_wizard.css"))"  />
    <link rel="stylesheet" type="text/css" href="@(this.Url.Content("~/Content/themes/redmond/jquery-ui.min.css"))" />
    <link rel="stylesheet" type="text/css" href="@(this.Url.Content("~/Content/jquery.jqGrid/redmond.ui.jqgrid.css"))" />
    <link rel="stylesheet" type="text/css" href="@(this.Url.Content("~/Content/JQuery.selectbox/jquery.selectbox.css"))" />
    <link rel="stylesheet" type="text/css" href="@(this.Url.Content("~/Content/Select2/select2-3.4.5.css"))" />
    <link rel="stylesheet" type="text/css" href="@(this.Url.Content("~/Content/datepicker.css"))" />
    <link rel="stylesheet" type="text/css" href="@(this.Url.Content("~/Content/font-awesome.min.css"))" />

    <style type="text/css">
        html, body
        {
            font-size: 85%;
        }

        .ui-inline-custom.ui-state-hover span
        {
            margin: -1px;
        }
    </style>

    @(this.RenderSection("Css", false))

    <script src="@(this.Url.Content("~/Scripts/Compatibility/all.js"))"> </script>
    <!--[if lt IE 9]>
            <link type="text/css" rel="stylesheet" href="@(this.Url.Content("~/Content/Compatibility/ltIE9.css"))" />
            <script src="@(this.Url.Content("~/Scripts/Compatibility/ltIE9.js"))"> </script>
            <script src="@(this.Url.Content("~/Scripts/Compatibility/html5shim.js"))"> </script>
            <script src="@(this.Url.Content("~/Scripts/RespondJS/respond.src.min.js"))"> </script>
        <![endif]-->
    <!--[if lt IE 9]>
            <script src="@(this.Url.Content("~/Scripts/Compatibility/json2.js"))"> </script>
        <![endif]-->

    <script src="@(this.Url.Content("~/Scripts/jquery-1.9.1.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/JQGrid/v4.4.4/i18n/grid.locale-en.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/JQGrid/v4.5.4/js/jquery.jqGrid.min.js"))"> </script>

    <script src="@(this.Url.Content("~/Scripts/jquery.validate.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/jquery.validate.unobtrusive.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/bootstrap.min.js"))"> </script>


    <script src="@(this.Url.Content("~/Scripts/Knockout/KnockOutBundle.min.js"))"> </script>

    <script src="@(this.Url.Content("~/Scripts/bootstrap-datepicker.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/jquery.blockUI.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/jquery-migrate-1.2.1.min.js"))"> </script>

    <script src="@(this.Url.Content("~/Scripts/moment.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/linq.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/typeahead.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/toastr.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/chosen.jquery.min.js"))"> </script>

    <script src="@(this.Url.Content("~/Scripts/App.min.js"))"> </script>

    <script src="@(this.Url.Content("~/Scripts/SmartWizard/jquery.smartWizard.min.js"))"> </script>
    <script src="@(this.Url.Content("~/Scripts/Select2/select2-3.4.5.min.js"))"> </script>

    <script src="@(this.Url.Content("~/Scripts/SuperFishMenu/superfish.min.js"))"> </script>

    <script type="text/javascript"> Reviewer.rootPath = '@(this.Url.Content("~"))'; </script>
</head>
<body style="min-height: 1024px; overflow: visible">

    <div id="topblackbar"></div>

    <div class="container">
        <header>
            <div class="navbar navbar-default" style="background-color: transparent; border-color: transparent;" role="navigation">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand visible-xs" href="#">@*logo*@
                    </a>

                </div>
                <div class="navbar-collapse collapse">
                    @* Header*@
                </div>
            </div>
        </header>
        <section id="main" class="row">
            @(this.RenderBody())
        </section>
        <footer class="row">
            <div class="col-md-6">
                <a href="http://www.icode.net/">ICode.net</a> | @ConfigurationManager.AppSettings["Environment"]          -       [Compiled @@ @File.GetCreationTime(Assembly.GetExecutingAssembly().Location)]   
            </div>
            <div class="col-md-6">
                <div class="pull-right">For internal use only. © ICode 2015 - Reviewer</div>
            </div>
        </footer>
    </div>


    <script type="text/javascript"> $(document).ready(function () { $.ajaxSetup({ cache: false }); @(this.RenderSection("JQueryOnReady", false)) }); </script>

    @* <hr />
    <br />
    <div data-bind="dump: $root"></div>*@
</body>
</html>
